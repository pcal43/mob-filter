{
  "schemaVersion": 1,
  "id": "mobfilter",
  "version": "${version}",
  "name": "Mob Filter",
  "description": "Rules-based filtering of mob spawns.",
  "authors": [ "pcal" ],
  "contact": {
    "homepage": "https://github.com/pcal43/",
    "sources": "https://github.com/pcal43/mob-filter"
  },
  "license": "MIT",
  "icon": "icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "net.pcal.mobfilter.FabricModInitializer"
    ],
